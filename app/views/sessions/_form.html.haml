= form_for :session, url: session_path do |form|
  .row
    .input-field.col.s12.m4
      = form.text_field :email, type: 'email'
      = form.label :email
  .row
    .input-field.col.s12.m4
      = form.password_field :password
      = form.label :password
  .row
    .input-field.col.s12
      = form.submit 'Sign In', class: 'btn primary waves-effect waves-light'

